    "react-hook-form": "^6.15.7",
    "react-redux": "^7.2.4",
    "react-scripts": "4.0.3",
    "react-sortablejs": "^6.0.0",
    "redux": "^4.0.5",
    "reselect": "^4.0.0",
    "sortablejs": "^1.13.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "7.14.0",
    "@babel/preset-env": "7.14.1",
    "@babel/preset-react": "7.13.13",
