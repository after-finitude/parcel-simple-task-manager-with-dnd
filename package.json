{
  "name": "simple-task-manager-with-dnd-parcel",
  "version": "1.0.0",
  "license": "MIT",
  "main": "src/index.tsx",
  "scripts": {
    "dev": "parcel src/index.html --open --no-source-maps --out-dir build/output --cache-dir build/cache",
    "build": "rimraf build && tsc --noEmit && parcel build src/index.html --no-source-maps --experimental-scope-hoisting --public-url ./ --out-dir build/output --cache-dir build/cache",
    "clean": "rimraf build node_modules"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react",
      "@babel/preset-typescript"
    ],
    "plugins": [
      [
        "module-resolver",
        {
          "root": [
            "./"
          ],
          "alias": {
            "components": "./src/components",
            "store": "./src/store"
          }
        }
      ]
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.10.2",
    "@material-ui/icons": "^4.9.1",
    "@reduxjs/toolkit": "^1.3.6",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hook-form": "^5.7.2",
    "react-redux": "^7.2.0",
    "react-scripts": "3.4.1",
    "react-sortablejs": "^2.0.11",
    "redux": "^4.0.5",
    "reselect": "^4.0.0",
    "store": "^2.0.12",
    "typescript": "^3.9.5",
    "uuid": "^8.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@babel/preset-typescript": "^7.10.1",
    "@types/node": "^14.0.13",
    "@types/react": "^16.9.36",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/styled-components": "^5.1.0",
    "@types/uuid": "^8.0.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "parcel": "^2.0.0-alpha.3.2",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-bundle-visualiser": "^1.2.0",
    "rimraf": "^3.0.2",
    "styled-components": "^5.1.1",
    "typescript": "^3.9.5"
  }
}
