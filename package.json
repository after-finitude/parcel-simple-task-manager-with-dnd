    "react-dom": "^17.0.1",
    "react-hook-form": "^6.13.1",
    "react-redux": "^7.2.2",
    "react-scripts": "4.0.1",
    "react-sortablejs": "^6.0.0",
    "redux": "^4.0.5",
    "reselect": "^4.0.0",
    "sortablejs": "^1.12.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@babel/preset-env": "7.12.11",
    "@babel/preset-react": "7.12.10",
    "@babel/preset-typescript": "7.12.7",
    "@types/enzyme": "^3.10.8",
    "@types/jest": "26.0.19",
    "@types/node": "14.14.16",
