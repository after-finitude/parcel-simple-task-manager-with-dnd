{
  "name": "simple-task-manager-with-dnd-parcel",
  "version": "1.0.0",
  "license": "MIT",
  "main": "src/index.tsx",
  "scripts": {
    "dev": "parcel src/index.html --open --no-source-maps --out-dir build/output",
    "build": "rimraf build && tsc --noEmit && parcel build src/index.html --no-source-maps --experimental-scope-hoisting --public-url ./ --out-dir build/output",
    "test": "jest",
    "test:cov": "jest --coverage",
    "clean": "rimraf build coverage node_modules"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react",
      "@babel/preset-typescript"
    ],
    "plugins": [
      [
        "module-resolver",
        {
          "root": [
            "./"
          ],
          "alias": {
            "components": "./src/components",
            "store": "./src/store"
          }
        }
      ]
    ]
  },
  "jest": {
    "testMatch": [
      "<rootDir>/src/**/*.(test).{ts,tsx}"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@reduxjs/toolkit": "^1.4.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hook-form": "^6.2.0",
    "react-redux": "^7.2.1",
    "react-scripts": "3.4.3",
    "react-sortablejs": "^2.0.11",
    "redux": "^4.0.5",
    "reselect": "^4.0.0",
    "typescript": "^3.9.7",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@babel/core": "7.11.1",
    "@babel/preset-env": "7.11.0",
    "@babel/preset-react": "7.10.4",
    "@babel/preset-typescript": "7.10.4",
    "@types/jest": "26.0.9",
    "@types/node": "14.0.27",
    "@types/react": "16.9.46",
    "@types/react-dom": "16.9.8",
    "@types/react-redux": "7.1.9",
    "@types/styled-components": "5.1.2",
    "@types/uuid": "8.3.0",
    "babel-plugin-module-resolver": "4.0.0",
    "jest": "26.4.0",
    "parcel": "latest",
    "parcel-plugin-bundle-visualiser": "1.2.0",
    "rimraf": "3.0.2",
    "styled-components": "5.1.1"
  }
}
