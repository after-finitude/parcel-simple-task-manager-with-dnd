{
  "name": "simple-task-manager-with-dnd-parcel",
  "version": "1.0.0",
  "license": "MIT",
  "main": "src/index.tsx",
  "scripts": {
    "dev": "parcel src/index.html --open --no-source-maps --out-dir build",
    "build": "rimraf build && tsc --noEmit && parcel build src/index.html --no-source-maps --experimental-scope-hoisting --public-url ./ --out-dir build",
    "lint": "eslint --ext .ts,.tsx src/",
    "pretty": "prettier --config .prettierrc.json --write src/**/*.{html,ts,tsx}",
    "test": "jest",
    "test:cov": "jest --coverage",
    "clean": "rimraf build node_modules"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@reduxjs/toolkit": "^1.4.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hook-form": "^6.8.2",
    "react-redux": "^7.2.1",
    "react-scripts": "3.4.3",
    "react-sortablejs": "^2.0.11",
    "redux": "^4.0.5",
    "reselect": "^4.0.0",
    "typescript": "^4.0.2",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@babel/core": "7.11.6",
    "@babel/preset-env": "7.11.5",
    "@babel/preset-react": "7.10.4",
    "@babel/preset-typescript": "7.10.4",
    "@types/enzyme": "^3.10.6",
    "@types/jest": "26.0.14",
    "@types/node": "14.10.2",
    "@types/react": "16.9.49",
    "@types/react-dom": "16.9.8",
    "@types/react-redux": "7.1.9",
    "@types/styled-components": "5.1.3",
    "@types/uuid": "8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "babel-plugin-module-resolver": "4.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.4",
    "enzyme-to-json": "^3.5.0",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.2",
    "jest": "26.4.2",
    "parcel": "latest",
    "parcel-plugin-bundle-visualiser": "1.2.0",
    "prettier": "2.1.2",
    "rimraf": "3.0.2",
    "styled-components": "5.2.0"
  }
}
